    <command:details>
      <command:name>Connect-AzDevOpsOrganization</command:name>
      <command:verb>Connect</command:verb>
      <command:noun>AzDevOpsOrganization</command:noun>
      <maml:description>
        <maml:para>This function will connect to Azure DevOps</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>This function will connect to Azure Devops, you will need to provide a valid Organization Name  and you must have created a Personal Access Token</maml:para>
    </maml:description>
